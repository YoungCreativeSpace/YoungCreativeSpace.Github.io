
/* Custom, iPhone Retina CSS //////////////////////*/
@media only screen and (min-width : 320px) {

/*navbar stuff*/
.navbar {
  background-color: black;
  padding: 2px 5px 6px 10px;
  border: 0 !important;
}


.navbar .navbar-nav {
  display: inline-block;
  float: none;
  vertical-align: top;
  border: 0 !important;
  font-size: 1.2em;
}

.navbar-header {
  font-weight: bold;
  font-family:'Open Sans', sans-serif;
  background-color: black;
  border: 0 !important;
}

.navbar-collapse {
  font-weight: bold;
  font-family:'Open Sans', sans-serif;
  background-color: black;
  text-align: center;
  border: 0 !important;
}

ul.nav a:hover { 
  color: gray !important;
}

.navbar .nav > li > a {
    color:  white;
}
/*end navbar stuff*/


/* text decoration stuff */
h1{
font-family:'Open Sans', sans-serif;
font-weight: bold;
font-size: 2.18em;
}

h3{
font-family:'Open Sans', sans-serif;
font-size: 1.4em;
font-weight: bold;
}

.lead{
  font-family:'Open Sans', sans-serif;
  font-size: 1.4em;
  font-weight: ;
}
/* end text decoration stuff */


/* instafeed stuff - populates index page with instagram pics  */
#instafeed{
  padding: .2px .2px .2px .2px;
  margin:10px;/*
  border:1px solid #e1e1e1;*/
  display:inline-block;
  border-radius: 4px;
  position:center;
 /* float: right;*/
}

#instafeed a {
/*  padding: .1px .1px .1px .1px;*/
  margin:5px;
/*  border:1px solid #e1e1e1;*/
  display:inline-block;
  display: inline-block;
  border-radius: 1px;
  position:center;
}
/* end instafeed stuff */


/* footer stuff */
.fa-stack {
  color: white;
  background-color: black
}

.fa-stack-2x {
  color: black;
}


.copyright {
  color: white;
}
/* end footer stuff */


/* link stuff*/
/* unvisited link */
a:link {
    color: black;
    text-decoration: none;
}
/* visited link */
a:visited {
    color: black;
    text-decoration: none;
}
/* mouse over link */
a:hover {
    color: black;
    text-decoration: none;
}
/* selected link */
a:active {
    color: black;
    text-decoration: none;
}
/* end link stuff*/


/* other stuff (self explanatory)*/
#athletePics{
    display:none;
}

#jbacLogo{
  width: 18em;
  height: 12em;
  padding-bottom: 3px;
}

/* Responsive iFrame for Calendar */
.responsive-iframe-container {
position: relative;
padding-bottom: 56.25%;
padding-top: 30px;
height: 0;
overflow: hidden;
}

.responsive-iframe-container iframe,
.vresponsive-iframe-container object,
.vresponsive-iframe-container embed {
position: absolute;
top: 0;
left: 0;
width: 100%;
height: 100%;
}
/* End Responsive iFrame for Calendar */


 body {
  background-color: white;
  margin:0; 
  padding:0;
  border: 0;
  outline: 0;
  vertical-align: top;
  overflow-x: hidden;
  display: block;
  width: 105%;
  }

header {
  background-color: black;
 /* overflow:auto;*/
  margin:0;
  padding:0;
  width: 105%;
  overflow-x: hidden
}

footer {
  background-color: black;
 /* overflow:auto;*/
  margin:0;
  padding:0;
  height: 150px;
  overflow-x: hidden;
  width: 105%;
}

.container-full {
  margin: 0 auto;
  width: 100%;
  overflow-x: hidden;
  width: 100%;
}

    }
/*End  Custom, iPhone Retina ////////////////////*/



